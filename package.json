{
  "name": "nilportugues-breakster",
  "version": "0.1.6",
  "description": "Tool that breaks your html into React components, saving their code into files.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p \"./src\" -w",
    "test": "tsc -p \"./src\" && ava ./tests/index.test.js --verbose",
    "test:generated": "jest ./tests/test-actual-output/components/*.test.tsx ",
    "test:i18n": " node ./src/lib/i18n/index.js  ./tests ./tests/out ./test-actual-output"
  },
  "repository": "https://github.com/nilportugues/breakster",
  "bin": "dist/index.js",
  "author": "nilportugues",
  "license": "MIT",
  "dependencies": {
    "async-file": "^2.0.2",
    "jsdom": "^9.12.0",
    "minimist": "^1.2.0",
    "@babel/core": "^7.4.4",
    "@babel/generator": "^7.4.4",
    "@babel/parser": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/traverse": "^7.4.4",
    "babel-project-relative-import": "^2.0.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/jsdom": "^2.0.30",
    "@types/minimist": "^1.2.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "ava": "^0.19.0",
    "del": "^2.2.2",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
