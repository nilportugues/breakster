{
  "name": "breakster",
  "version": "0.1.6",
  "description": "Tool that breaks your html into `reacty` (React, Preact) components, saving their code into files. Imports of children components, JSX with children components, etc - you're all set.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p \"./src\" -w",
    "test": "tsc -p \"./src\" && ava ./tests/test.js --verbose"
  },
  "repository": "https://github.com/nilportugues/breakster",
  "bin": "dist/index.js",
  "author": "idchlife",
  "license": "MIT",
  "dependencies": {
    "async-file": "^2.0.2",
    "jsdom": "^9.12.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/jsdom": "^2.0.30",
    "@types/minimist": "^1.2.0",
    "@types/node": "^18.11.9",
    "ava": "^0.19.0",
    "del": "^2.2.2",
    "react": "^18.2.0",
    "typescript": "^4.9.3"
  },
  "preferGlobal": true
}
